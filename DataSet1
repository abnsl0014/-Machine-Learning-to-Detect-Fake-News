    "def lemmatize(title):\n",
    "    title2 = 'title -' + str(title).lower()\n",
    "    words = TextBlob(title).words\n",
    "    # for each word, take its \"base form\" = lemma \n",
    "    return [word.lemma for word in words]\n",
    "\n",
    "adv.title.head().apply(lemmatize)"
